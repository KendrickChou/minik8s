{
    "kind": "ReplicaSet",
    "apiversion": "v1",
    "metadata": {
        "name": "test-replica",
        "namespace": "test",
        "labels": {
            "app": "mysql"
        }
    },
    "spec": {
        "replicas": 3,
        "type": "ClusterIP",
        "clusterIP": "10.1.1.1",
        "selector":{
            "matchLabels": {
                "app": "mysql"
            }
        },
        "template":{
            "metadata": {
                "name": "test-pod-1",
                "namespace": "test",
                "uid": "",
                "labels": {
                    "app" : "mysql"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "test-container-1",
                        "namespace": "test",
                        "image": "mysql:latest",
                        "imagepullpolicy": "IfNotPresent"
                    }
                ]
            }
        }
    }
}