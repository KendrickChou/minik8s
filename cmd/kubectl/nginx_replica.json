{
    "kind": "ReplicaSet",
    "apiversion": "v1",
    "metadata": {
        "name": "nginx-replica-test",
        "namespace": "test",
        "labels": {
            "app": "nginx"
        }
    },
    "spec": {
        "replicas": 3,
        "type": "ClusterIP",
        "clusterIP": "10.1.1.1",
        "selector":{
            "matchLabels": {
                "app": "nginx"
            }
        },
        "template":{
            "metadata": {
                "name": "test-pod-1",
                "namespace": "test",
                "uid": "",
                "labels": {
                    "app" : "nginx"
                }
            },
            "spec": {
                "Containers": [
                    {
                        "Name": "test-container-1",
                        "Namespace": "test",
                        "Image": "nginx:latest",
                        "ImagePullPolicy": "IfNotPresent"
                    }
                ]
            }
        }
    }

}